import java.util.Locale;
import java.util.Scanner; 
import java.util.Locale; 
public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US); 
		Scanner sc = new Scanner (System.in); 
		
		double A; 
		double notas100;
		double notas50;
		double notas20; 
		double notas10;
		double notas5; 
		double notas2; 
		double moedas1;
		double moedas50; 
		double moedas25;
		double moedas10;
		double moedas5; 
		double moedas1c; 
		
		A= sc.nextDouble(); 
		
		
	notas100= (int)A/100; 
	notas50= (int)(A%100)/50; 
	notas20= (int)((A%100)%50)/20; 
	notas10= (int)(((A%100)%50)%20)/10; 
	notas5= (int)((((A%100)%50)%20)%10)/5;
	notas2= (int)(((((A%100)%50)%20)%10)%5)/2; 
	moedas1=(int)((((((A%100)%50)%20)%10)%5)%2)/1;
	moedas50= (int)(((((((A%100)%50)%20)%10)%5)%2)%1)/0.50;
	moedas25=(int)((((((((A%100)%50)%20)%10)%5)%2)%1)%0.50)/0.25;
	moedas10= (int)(((((((((A%100)%50)%20)%10)%5)%2)%1)%0.50)%0.25)/0.10; 
	moedas5= (int)((((((((((A%100)%50)%20)%10)%5)%2)%1)%0.50)%0.25)%0.10)/0.05;
	moedas1c=(int)(((((((((((A%100)%50)%20)%10)%5)%2)%1)%0.50)%0.25)%0.10)%0.05)/0.01;
	
	System.out.printf("NOTAS:%n"); 
	System.out.printf("%d nota(s) de R$ 100.00%n", notas100); 
	System.out.printf("%d nota(s) de R$ 50.00%n", notas50);
	System.out.printf("%d nota(s) de R$ 20.00%n", notas20);
	System.out.printf("%d nota(s) de R$ 10.00%n", notas10); 
	System.out.printf("%d nota(s) de R$ 5.00%n", notas5);
	System.out.printf("%d nota(s) de R$ 2.00%n", notas2); 
	
	System.out.printf("MOEDAS:%n"); 
	System.out.printf("%d moedas(s) de R$ 1.00", moedas1); 
	System.out.printf("%d moedas(s) de R$ 0.50", moedas50); 
	System.out.printf("%d moedas(s) de R$ 0.25", moedas25); 
	System.out.printf("%d moedas(s) de R$ 0.10", moedas10); 
	System.out.printf("%d moedas(s) de R$ 0.05", moedas5); 
	System.out.printf("%d moedas(s) de R$ 0.01", moedas1c);  
	
		
		sc.close(); 
	}

}

