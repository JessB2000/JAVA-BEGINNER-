import java.util.Scanner; 
public class Main {

	public static void main(String[] args) {
Scanner sc = new Scanner (System.in); 

int s; 
int i=0; 

s=sc.nextInt(); 

while (s!=2002) {
	
	System.out.printf("Senha Inválida"); 
	
	s=sc.nextInt(); 
	
	if (s==2002) {
		
		System.out.printf("Acesso permitido"); 
		break; 
	}
	
	i++; 
}

sc.close(); 
	}

}
