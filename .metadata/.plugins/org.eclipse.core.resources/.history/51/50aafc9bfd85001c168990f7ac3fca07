import java.util.Scanner; 
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in); 
		
		int N; 
		int i; 
		
		int final1 = 1; 
		int fatorial; 
		
		N=sc.nextInt(); 
		
		for (i=0; i<=N; i++) {
			
			N *= (N-i); 
		}
	
		
      System.out.printf("%d%n", N);  
      
      sc.close(); 
	
	}

}
